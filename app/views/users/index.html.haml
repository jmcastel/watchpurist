.jumbotron.text-center
	.h1 All Users
.container
	- @users.each do |user|
		
		.col-md-12.filter
			
			
			.col-md-6.img
				.avatar= link_to (image_tag  user.avatar.url(:thumb)) , single_user_path(user), :class => "img-circle"
			.col-md-6
				%h2= link_to "#{user.pseudo}" ,single_user_path(user)
		%br	
		%br
%br
%br			